{
    "name": "ai-note-taking-app",
    "version": "1.0.0",
    "description": "AI-Enhanced Note-Taking Application",
    "private": true,
    "workspaces": [
      "frontend",
      "backend"
    ],
    "scripts": {
      "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
      "start:frontend": "npm run start --workspace=frontend",
      "start:backend": "npm run start --workspace=backend",
      "install:all": "npm install && npm install --workspace=frontend && npm install --workspace=backend",
      "build": "npm run build --workspace=frontend",
      "test": "npm run test --workspace=frontend && npm run test --workspace=backend"
    },
    "keywords": [
      "note-taking",
      "ai",
      "productivity"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
      "concurrently": "^8.2.0"
    }
  }
